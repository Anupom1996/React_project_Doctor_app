/*loader for Home Start */
.loaderH-outer{
	left:0;
	top:0;
	position:fixed;
	height:100%;
	width:100%;
	background:#391D97;
    z-index: 1000000;
}
.loaderH{
	position:fixed;
	left:0%;
    top:0%;
    height:100%;
	width:100%;
}
@-webkit-keyframes ball-triangle-ani-1 {
    33% {
      -webkit-transform: translate(75px, -120px);
              transform: translate(75px, -120px); }
    66% {
      -webkit-transform: translate(120px, 0px);
              transform: translate(120px, 0px); }
    100% {
      -webkit-transform: translate(0px, 0px);
              transform: translate(0px, 0px); } }
  
@keyframes ball-triangle-ani-1 {
    33% {
      -webkit-transform: translate(75px, -120px);
              transform: translate(75px, -120px); }
    66% {
      -webkit-transform: translate(120px, 0px);
              transform: translate(120px, 0px); }
    100% {
      -webkit-transform: translate(0px, 0px);
              transform: translate(0px, 0px); } }
  
@-webkit-keyframes ball-triangle-ani-2 {
    33% {
      -webkit-transform: translate(75px, 120px);
              transform: translate(75px, 120px); }
    66% {
      -webkit-transform: translate(-75px, 120px);
              transform: translate(-75px, 120px); }
    100% {
      -webkit-transform: translate(0px, 0px);
              transform: translate(0px, 0px); } }
  
@keyframes ball-triangle-ani-2 {
    33% {
      -webkit-transform: translate(25px, 50px);
              transform: translate(25px, 50px); }
    66% {
      -webkit-transform: translate(-25px, 50px);
              transform: translate(-25px, 50px); }
    100% {
      -webkit-transform: translate(0px, 0px);
              transform: translate(0px, 0px); } }
  
@-webkit-keyframes ball-triangle-ani-3 {
    33% {
      -webkit-transform: translate(-50px, 0px);
              transform: translate(-50px, 0px); }
    66% {
      -webkit-transform: translate(-25px, -50px);
              transform: translate(-25px, -50px); }
    100% {
      -webkit-transform: translate(0px, 0px);
              transform: translate(0px, 0px); } }
  
@keyframes ball-triangle-ani-3 {
    33% {
      -webkit-transform: translate(-50px, 0px);
              transform: translate(-50px, 0px); }
    66% {
      -webkit-transform: translate(-25px, -50px);
              transform: translate(-25px, -50px); }
    100% {
      -webkit-transform: translate(0px, 0px);
              transform: translate(0px, 0px); } }
  
.ball-triangle-ani {
    position: relative;
    -webkit-transform: translate(-29.994px, -37.50938px);
    transform: translate(-29.994px, -37.50938px); 
    opacity: 0.2;
}
.ball-triangle-ani > div:nth-child(1) {
    -webkit-animation-name: ball-triangle-ani-1;
            animation-name: ball-triangle-ani-1;
    -webkit-animation-delay: 0;
            animation-delay: 0;
    -webkit-animation-duration: 2s;
            animation-duration: 2s;
    -webkit-animation-timing-function: ease-in-out;
            animation-timing-function: ease-in-out;
    -webkit-animation-iteration-count: infinite;
            animation-iteration-count: infinite; 
}
.ball-triangle-ani > div:nth-child(2) {
    -webkit-animation-name: ball-triangle-ani-2;
            animation-name: ball-triangle-ani-2;
    -webkit-animation-delay: 0;
            animation-delay: 0;
    -webkit-animation-duration: 2s;
            animation-duration: 2s;
    -webkit-animation-timing-function: ease-in-out;
            animation-timing-function: ease-in-out;
    -webkit-animation-iteration-count: infinite;
            animation-iteration-count: infinite; 
}
.ball-triangle-ani > div:nth-child(3) {
    -webkit-animation-name: ball-triangle-ani-3;
            animation-name: ball-triangle-ani-3;
    -webkit-animation-delay: 0;
            animation-delay: 0;
    -webkit-animation-duration: 2s;
            animation-duration: 2s;
    -webkit-animation-timing-function: ease-in-out;
            animation-timing-function: ease-in-out;
    -webkit-animation-iteration-count: infinite;
            animation-iteration-count: infinite; 
}
.ball-triangle-ani > div {
    -webkit-animation-fill-mode: both;
    animation-fill-mode: both;
    position: absolute;
    border-radius: 100%;
}
.ball-triangle-ani > div:nth-of-type(1) {
    top: 80px;
    left: 42px;
}
.ball-triangle-ani > div:nth-of-type(2) {
    top: 40px;
    left: 450px;
}
.ball-triangle-ani > div:nth-of-type(3) {
    top: 400px;
    left: 400px;
}
.ball-triangle-ani > div:first-child{
    border: 12px solid  #fad20a;
    width:300px;
    height:300px;
}
.ball-triangle-ani > div:nth-child(2){
    border: 12px solid #5f4ba0;
    width:200px;
    height:200px;
}
.ball-triangle-ani > div:last-child{
    border: 12px solid #31c8f4;
    width:150px;
    height:150px;
}
.mainpage{
    -webkit-animation: fadein 1s linear forwards;
    animation: fadein 1s linear forwards;
    opacity: 0;
}
@-webkit-keyframes fadein {
	0% { opacity: 0; }
	50% { opacity: 0.5; }
	100% { opacity: 1; }
}
  
@keyframes fadein {
	0% { opacity: 0; }
	50% { opacity: 0.5; }
	100% { opacity: 1; }
}
/*loader for Home End */
.mainpage .container{
    max-width: 1200px;
}
/* Arrows */
.mainpage .slick-next,
.mainpage .slick-prev {
	font-size: 0;
	line-height: 0;
	position: absolute;
	top: 50%;
	display: block;
    width: 40px;
	height: 22px;
	padding: 0;
	-webkit-transform: translate(0, -50%);
	-ms-transform: translate(0, -50%);
	transform: translate(0, -50%);
	cursor: pointer;
	color: transparent;
	border: none;
	outline: none;
	background: transparent;
	z-index: 1;
}
.mainpage .slick-next:hover,  .mainpage.slick-next:focus {
	color: transparent;
	outline: none;
    background: transparent;
    -webkit-transform: translate(10px, -50%);
	-ms-transform: translate(10px, -50%);
	transform: translate(10px, -50%);
}
.mainpage .slick-prev:hover,  .mainpage.slick-prev:focus {
	color: transparent;
	outline: none;
    background: transparent;
    -webkit-transform: translate(-10px, -50%);
	-ms-transform: translate(-10px, -50%);
	transform: translate(-10px, -50%);
}
.mainpage .slick-next:hover:before, .mainpage .slick-next:focus:before,
.mainpage .slick-prev:hover:before, .mainpage .slick-prev:focus:before {
	opacity: 1;
}
.mainpage .slick-next.slick-disabled:before {
	opacity: .25;
}
.mainpage .slick-prev.slick-disabled:before {
	opacity: .25;
}
.mainpage .slick-next {
    right: 5px;
    transition: all 0.5s ease;
}
.mainpage .slick-prev {
    left: 5px;
    transition: all 0.5s ease;
}
.mainpage .slick-next:before {
    width: 40px;
	height: 22px;
	content: '';
	background: transparent url("../images/slick-next-main.svg") no-repeat 0 0;
	display: block;
}
.mainpage .slick-prev:before {
    width: 40px;
	height: 22px;
	content: '';
	background: transparent url("../images/slick-prev-main.svg") no-repeat 0 0;
	display: block;
}
