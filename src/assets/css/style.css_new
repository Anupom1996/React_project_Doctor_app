    box-shadow: 0px 0px 10px 0px rgba(0,0,0,0.75);
    transition-delay: 0.8s;
} 
/* Animation Start*/
.howworksCont figure {
	border: 0;
	box-sizing: border-box;
	box-shadow: inset 0 0 0 2px #BFB0F3;
	position: relative;
	vertical-align: middle;
	border-radius: 100%;
	box-shadow: none;
}
.howworksCont figure::before, .howworksCont figure::after {
	 box-sizing: inherit;
	 content: '';
	 position: absolute;
	 width: 100%;
	 height: 100%;
	 border-radius: 100%;
}
.howworksCont .spin {
	width: 250px;
    height: 250px;
    padding: 0;
    -ms-transform: rotate(-80deg); /* IE 9 */
    -webkit-transform: rotate(-80deg); /* Safari prior 9.0 */
    transform: rotate(-80deg); /* Standard syntax */
}
.howworksCont .spin img{
    -ms-transform: rotate(80deg); /* IE 9 */
    -webkit-transform: rotate(80deg); /* Safari prior 9.0 */
    transform: rotate(80deg); /* Standard syntax */
}
.howworksCont .spin::before, .howworksCont .spin::after {
	 top: 0;
	 left: 0;
}
.howworksCont .spin::before {
    border: 2px dashed transparent;
}
.howworksCont .spin:hover::before,
.howworksContSin:hover .spin::before {
	 border-top-color: #BFB0F3;
	 border-right-color: #BFB0F3;
	 border-bottom-color: #BFB0F3;
	 -webkit-transition: border-top-color 0.25s linear, border-right-color 0.25s linear 0.2s, border-bottom-color 0.25s linear 0.3s;
	 transition: border-top-color 0.25s linear, border-right-color 0.25s linear 0.2s, border-bottom-color 0.25s linear 0.3s;
}
.howworksCont .spin::after {
 	border: 0 solid transparent;
}
.spin:hover::after,
.howworksContSin:hover .spin::after {
	 border-top: 2px dashed #BFB0F3;
	 border-left-width: 2px;
	 border-right-width: 2px;
	 -webkit-transform: rotate(270deg);
	 transform: rotate(270deg);
	 -webkit-transition: border-left-width 0s linear 0.45s, -webkit-transform 0.6s linear 0s;
	 transition: border-left-width 0s linear 0.45s, -webkit-transform 0.6s linear 0s;
	 transition: transform 0.6s linear 0s, border-left-width 0s linear 0.45s;
	 transition: transform 0.6s linear 0s, border-left-width 0s linear 0.45s, -webkit-transform 0.6s linear 0s;
}
/* Animation End*/
