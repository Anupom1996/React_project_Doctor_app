.colsebtn{
    position: absolute;
    top: -40px;
    right: -40px;
}
.colsebtn .outer {
    position: relative;
    margin: auto;
    width: 30px;
    cursor: pointer;
}
.colsebtn .inner {
    width: inherit;
    text-align: center;
  }
 .colsebtn  label { 
    font-size: .8em; 
    line-height: 3em;
    text-transform: uppercase;
    color: #fff;
    transition: all .3s ease-in;
    opacity: 0;
    cursor: pointer;
    margin: 0;
}
.colsebtn .inner:before, .inner:after {
    position: absolute;
    content: '';
    height: 2px;
    width: inherit;
    background: #fff;
    left: 0;
    transition: all .3s ease-in;
}
.colsebtn .inner:before {
    top: 50%; 
    transform: rotate(45deg);  
}
.colsebtn  .inner:after {  
    bottom: 50%;
    transform: rotate(-45deg);  
}
.colsebtn  .outer:hover label {
    opacity: 1;
}
.colsebtn  .outer:hover .inner:before,
  .outer:hover .inner:after {
    transform: rotate(0);
}
.colsebtn .outer:hover .inner:before {
    top: 0;
}
.colsebtn .outer:hover .inner:after {
    bottom: 0;
}
/* Button Style Start*/
.button {
  cursor: pointer;
}
.button span {
  cursor: pointer;
  display: inline-block;
  position: relative;
  transition: 0.5s;
}
.button span:after {
  content: '\00bb';
  position: absolute;
  opacity: 0;
  top: 0;
  right: -20px;
  transition: 0.5s;
}
.button:hover span {
    padding-right: 25px;
}
.button:hover span:after {
  opacity: 1;
  right: 0;
}
/* Button Style End*/
