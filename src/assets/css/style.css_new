.new-banner .owl-dots { counter-reset: slides-num; position: absolute; top:0; left: -100px; }
.new-banner .owl-dots .owl-dot.active:after { content: counter(slides-num); /* get total number of items*/ color: #00c7ff; font-size: 12px; line-height: 20px; position: absolute; right: -30px; top: -5px;} 
.new-banner.owl-theme .owl-dots .owl-dot.active{ position: relative;}
.new-banner.owl-theme .owl-dots .owl-dot{ display: block; counter-increment: slides-num; /* Increment counter */}
